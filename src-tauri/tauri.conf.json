{
	"$schema": "https://schema.tauri.app/config/2",
	"productName": "auto-shutdown",
	"version": "0.1.0",
	"identifier": "com.auto-shutdown.app",
	"build": {
		"beforeDevCommand": "npm run dev",
		"devUrl": "http://localhost:1420",
		"beforeBuildCommand": "npm run build",
		"frontendDist": "../dist"
	},
	"app": {
		"windows": [
			{
				"title": "auto-shutdown",
				"width": 800,
				"height": 1080,
				"decorations": false,
				"transparent": true,
				"minHeight": 600,
				"minWidth": 400
			}
		],
		"security": {
			"csp": null
		},
		"withGlobalTauri": true
	},
	"bundle": {
		"active": true,
		"targets": "all",
		"icon": [
			"icons/32x32.png",
			"icons/128x128.png",
			"icons/128x128@2x.png",
			"icons/icon.icns",
			"icons/icon.ico"
		]
	}

}
